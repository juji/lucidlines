    "lint-staged": "^16.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/http-proxy": "^1.17.16",
    "cp-dir": "^1.0.3",
    "http-proxy": "^1.18.1",
    "json5": "^2.2.3",
    "lokijs": "^1.5.12",
    "object-code": "^1.3.4",
    "tmp": "^0.2.5",
    "ws": "^8.18.3"
  }
