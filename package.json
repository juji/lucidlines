  },
  "homepage": "https://github.com/juji/lucidlines#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "typescript",
    "library",
    "nodejs"
  ],
  "scripts": {
    "prepare": "cd client && npm install && cd ../ && husky || true",
    "build:client": "cd client && npm install && npm run build && cp-dir dist ../dist/dash",
